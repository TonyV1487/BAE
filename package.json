{
  "name": "BAE",
  "version": "1.0.0",
  "description": "Everyone has been in meetings that were uneccessary and shuold have been emails. Bae is a full MERN stack webapp that aims to streamline the process of planing meetings and determining if it should actually be an email.",
  "main": "index.js",
  "dependencies": {
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "express-secure-only": "^0.2.1",
    "express-session": "^1.17.0",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "mongoose": "^5.9.2",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "scripts": {
    "client-install": "npm install ./client/package.json",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon ./server/index.js",
    "client": "npm start ./client/src/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
